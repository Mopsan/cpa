{
    "name": "artjoker/cpa",
    "description": "Laravel CPA (Cost Per Action) network integration package",
    "license": "MIT",
    "authors": [
        {
            "name": "VT2",
            "email": "v.taranenko@artjoker.net",
            "homepage": "https://github.com/artjoker"
        }
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/artjoker/cpa",
    "keywords": ["Laravel", "Cpa", "cost per action", "sales doubler", "storm digital", "admitad", "leads su", "do affiliate", "credy", "papa karlo", "fin line", "lead gid", "loangate", "Appscorp", "PAP"],
    "require": {
