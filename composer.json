    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^3.0|^4.0|^5.0|^6.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Artjoker\\Cpa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artjoker\\Cpa\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artjoker\\Cpa\\CpaServiceProvider"
            ],
            "aliases": {
                "Cpa": "Artjoker\\Cpa\\Facades\\Cpa"
            }
        }
    }
